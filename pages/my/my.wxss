  width: 72rpx;
  height:72rpx;
}
.coupon-all>view:nth-of-type(2) ,.coupon-use>view:nth-of-type(2) ,.coupon-used>view:nth-of-type(2),.overdue>view:nth-of-type(2){
  font-size: 30rpx;
  line-height: 1;
  font-family:PingFangSC-Medium,PingFang SC;
  font-weight:500;
  color:rgba(51,51,51,1);
  margin-top: 20rpx;
}
.coupon-use{
  margin-left: 42rpx;
}
.coupon-use>view:nth-of-type(1){
   width:60rpx;
   height:72rpx;
    margin: 0 auto;
}
.coupon-use image{
  width:60rpx;
  height:72rpx;
}
.coupon-used{
  margin-left: 40rpx;
}
.coupon-used>view:nth-of-type(1){
  width:60rpx;
  height:72rpx;
   margin: 0 auto;
}
.coupon-used image{
   width:60rpx;
   height:72rpx;
}
.overdue{
    margin-left: 40rpx;
}
.overdue>view:nth-of-type(1){
    width: 60rpx;
    height: 72rpx;
     margin: 0 auto;
}
.overdue image{
    width: 60rpx;
    height: 72rpx;
}
.border{
    width: 2rpx;
    height: 101rpx;
    background:#fff;
    margin-left:50rpx;
}

