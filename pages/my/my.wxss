/* footer */
.footer{
  width: 100%;
  height: 128rpx;
  position: fixed;
  border-top: 2rpx solid #D6D6D6;
  bottom: 0;
  left: 0;
  display: flex;
  background: #fff;
  z-index: 5;
  justify-content: space-around;
  text-align: center;
}
.myOrder{
  margin:0 30rpx;
  height:90rpx;
  background:rgba(250,250,250,1);
  border-radius:12rpx;
  display: flex;
  justify-content: space-between;
  margin-top: 55rpx;
  align-items: center;
}
.myOrder>view:nth-of-type(1){
  margin-left: 30rpx;
  font-size:30rpx;
  font-family:PingFangSC-Medium,PingFang SC;
  font-weight:500;
  color:rgba(51,51,51,1);
}
.myOrder>view:nth-of-type(2){
  margin-right: 30rpx;
}
.myOrder>view:nth-of-type(2) image{
  width:13rpx;
  height:25rpx;
}
.helpCenter{
  margin:0 30rpx;
  height:90rpx;
  background:rgba(250,250,250,1);
  border-radius:12rpx;
  display: flex;
  justify-content:space-between;
  margin-top: 30rpx;
  align-items: center;
}
.helpCenter>view:nth-of-type(1){
  margin-left: 30rpx;
  font-size:30rpx;
  font-family:PingFangSC-Medium,PingFang SC;
  font-weight:500;
  color:rgba(51,51,51,1);
}
.helpCenter>view:nth-of-type(2) image{
  width:13rpx;
  height:25rpx;
}
.helpCenter>view:nth-of-type(2){
  margin-right: 30rpx;
}
