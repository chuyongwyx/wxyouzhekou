}
/* 头像 */
.user-login ,.user-logined{
  margin-left: 54rpx;
  display: flex;
  margin-top: 90rpx;
  align-items: center;
  font-size:34rpx;
  font-family:PingFangSC-Medium,PingFang SC;
  font-weight:500;
  color:rgba(255,255,255,1);
}
.user-login>view:nth-of-type(1) ,.user-logined>view:nth-of-type(1){
  width: 122rpx;
  height: 125rpx;
  margin-right: 20rpx;
}
.user-login>view:nth-of-type(1)>image ,.user-logined>view:nth-of-type(1)>image{
    width: 122rpx;
    height: 125rpx;
}
.user-logined>view:nth-of-type(2)>view:nth-of-type(1){
