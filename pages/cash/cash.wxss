.cash-input{
   height: 90rpx;
   line-height: 90rpx;
   width: 650rpx;
}
.cash-input>input{
   height: 90rpx;
   line-height: 90rpx;
   width: 650rpx;
   font-size: 62rpx;
   font-weight: bold;
   font-family: 'DIN Alternate';
}

