<!--pages/incomeDetails/incomeDetail.wxml-->
<view>
     <view class="income-head" style="padding-top:{{statusBarHeight}}px;">
            <view class="income-back" bindtap="handleToBack"><image src="../../images/icon_fanhui.png"></image>                 </view>
            <view class="income-h1">提现</view>
      </view>
      <view class="main" style="margin-top:{{statusBarHeight*2+90}}rpx;">
      <!-- 累计收入 -->
      <view class="count">
            <view>累计收入</view>
            <view>{{total_commission?total_commission:'0'}}</view>
      </view>
      <!-- 提现 -->
      <view class="able-cash">
            <view>
                  <view>可提现</view>
                  <view>{{commission?commission:'0'}}</view>
            </view>
            <view>
                  <view>待到账</view>
                  <view>{{total_arrival?total_arrival:'0'}}</view>
            </view>
      </view> 
      <!-- 收支记录 -->
      <view class="collect">
             <view>收支记录</view>
             <view>
                  <view class="{{all?'active':'deactive'}}" bindtap="handletotype" id="0">全部</view>
                  <view class="{{get?'active':'deactive'}}" bindtap="handletotype" id="1">收入</view>
                  <view class="{{await?'active':'deactive'}}" bindtap="handletotype" id="2">待到账</view>
                  <view class="{{comtrue?'active':'deactive'}}"bindtap="handletotype" id="3">提现</view>
             </view>
      </view>
      <!-- 记录列表 -->
      <view class="list">
          <view class="list-content" wx:for="{{dataList}}" wx:key="{{this}}">
                  <view>
                        <view>{{item.title}}</view>
                        <view>{{item.create_time}}</view>
                  </view>
                  <view>{{item.price}}</view>
          </view> 
          
      </view> 
      </view>
</view>
